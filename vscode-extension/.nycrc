{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/**/*.ts"
  ]
}
