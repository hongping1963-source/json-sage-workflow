    "onnxruntime-web": "^1.16.3",
    "sharp": "^0.33.2",
    "zod": "^3.22.4",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.8",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
