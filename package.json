{
  "name": "@zhanghongping/smolvlm",
  "version": "1.0.0",
  "description": "A lightweight visual AI package based on SmolVLM model with complete toolchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "workspaces": [
    "cli"
  ],
  "scripts": {
    "test": "jest test/vision/",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run test:all && npm run lint",
    "docs": "typedoc",
    "download:models": "node scripts/download-models.js",
    "build:wasm": "node scripts/build-wasm.js",
    "optimize": "node scripts/optimize-model.js",
    "benchmark": "ts-node test/benchmark/index.ts",
    "type-check": "tsc --noEmit",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hongping1963-source/json-sage-workflow.git"
  },
  "keywords": [
    "ai",
    "vision",
    "machine-learning",
    "onnx",
    "tensorflow",
    "tflite",
    "torchscript",
    "optimization",
    "edge-computing"
  ],
  "author": "Zhang Hongping",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hongping1963-source/json-sage-workflow/issues"
  },
  "homepage": "https://github.com/hongping1963-source/json-sage-workflow#readme",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.17.0",
    "onnxruntime-node": "^1.17.0",
    "sharp": "^0.33.2",
    "torch-js": "^0.15.0",
    "winston": "^3.11.0",
    "axios": "^1.6.5",
    "lru-cache": "^10.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.17.14",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "@size-limit/preset-small-lib": "^11.0.2",
    "size-limit": "^11.0.2",
    "benchmark": "^2.1.4",
    "@types/benchmark": "^2.1.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "500 KB"
    }
  ]
}
